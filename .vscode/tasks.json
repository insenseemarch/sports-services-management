{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "dotnet build",
			"type": "shell",
			"command": "dotnet",
			"args": [
				"build",
				"webapp-mvc"
			],
			"problemMatcher": [
				"$msCompile"
			],
			"group": "build"
		},
		{
			"label": "dotnet run",
			"type": "shell",
			"command": "dotnet",
			"args": [
				"run",
				"webapp-mvc"
			],
			"isBackground": true,
			"group": "build"
		}
	]
}